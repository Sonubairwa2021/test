@model IEnumerable<InterviewProject.Controllers.states>


@Html.ValidationSummary(true, "", new { @class = "text-danger" })
<div class="form-group">
    @Html.Label("state", htmlAttributes: new { @class = "control-label col-md-2" })
    <div class="col-md-10">
        @Html.DropDownList("state", new SelectList(Model, "id", "state"), "Select state", new { @Class = "form-control" } )
        @Html.ValidationMessage("state", "", new { @class = "text-danger" })
    </div>
</div>


